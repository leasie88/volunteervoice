<%= javascript_include_tag "/assets/jquery.js"%>

<style>
.container {
	margin-top:30px;
}
.top {
	text-align:center;
}
.blurb {
	margin-top:50px;
	font-family:JosefinSlabRegular; 
	color:#9bce6c; 
	font-size:2em;
}
.submitContainer{
	text-align:center; 
	margin-top:75px;
}
</style>

<div class="container">
	<div class="row">
		<div class="twelve columns" class="top">

		<%= image_tag "https://s3-us-west-1.amazonaws.com/volunteervoice/images/thanksInTouch.png"%>
		
		<p class="blurb">We really appreciate your feedback! </p>
		
		
<div class="submitContainer">		
<%= link_to (image_tag "https://s3-us-west-1.amazonaws.com/volunteervoice/images/logo.png"), root_path, :onmousedown => "this.className = 'shadowed'", :onmouseup => "this.className = 'nothing'"%>
</div>
</div>
</div>
