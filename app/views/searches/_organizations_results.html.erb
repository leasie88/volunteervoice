<div style="padding:0; margin:0; width:100%" onclick="$('#org<%=f.id%>').click();" style="cursor:pointer">
	<table style="border-bottom:1px solid #575758; border-right:1px solid #575758; background-color:#E9ECE9; height:110px;"width="100%">
		<tr style="background-color:#E9ECE9">
			<td width="7%"><p style="margin-top:7px"><%=image_tag f.square_image.url, :style => "height:75px;width:75px" %></p></td>
			<td><p style="margin-top:7px"><a style="font-size:2em;" class="search_link" href="/organizations/<%=f.id%>"><%= f.name%></a></p></td>
			<td><p style="text-align:right"><%=render :partial => 'star', :locals => {:star => f.overall}%></p><p style="text-align:right; margin-top:-10px; font-family:architectsDaughterRegular; color: #e67b5a; font-size:.8em;">Based on <%=f.reviews_count%> reviews</p></td>
		</tr>
		<tr style="background-color:#E9ECE9">
			<td></td>
			<td colspan="2" style="width:60%"><p style="margin-top:-80px; font-size:1.3em;color:#e67b5a">
			<% count = 1%>
			<% f.programs.each do |p| %>
				<%=link_to p.name, p, :class => "programLink"%><%=", " unless count == f.programs.count%>
				<% count = count + 1%>
			<% end%>
				</p></td>
		</tr>
	</table>


</div>
