<%= simple_form_for(@search) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :region %>
    <%= f.input :subjects %>
    <%= f.input :lengths %>
    <%= f.input :sizes %>
    <%= f.input :price_min %>
    <%= f.input :price_max %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
