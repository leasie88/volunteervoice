	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<title>Volunteer Voice</title>
	<%= csrf_meta_tag %>
	<%= stylesheet_link_tag "application", :media => :all%>
	<%= javascript_include_tag "application"%>


	</head>

	<body>
		<!-- Header-->
		<div class="container" id="header" style="min-width:970px; background:url(/assets/headerFooter.png)">
		<header class="row">
			
			<div class="four columns" style="margin-top:15px">
			<a href="/" style="cursor:pointer"><img src="https://s3-us-west-1.amazonaws.com/volunteervoice/images/logo.png" alt="VolunteerVoice International logo" name="logo" style="height:95px"></a>
			</div>
			
			
			<div class="four columns" style="float:right">
			<div class="row login" style="width:auto">

				<% if !(user_signed_in?) && !(organization_account_signed_in?) %>
				<a href="#login-box" class="login-window">LOGIN</a>
				or <a href= "/sign_up" class="register">JOIN</a>
				<% else %>
					<% if user_signed_in?%>
						<a href="/pages/profile"> <%=current_user.username%> 
							<% if current_user.unread_messages > 0 %>
								<font style="color:#e67b5a">(<%=current_user.unread_messages%>)</font><% end %></a>&nbsp;&nbsp;
						<a href= "/signout" class="logout">LOGOUT</a>
					<% end %>
					<% if organization_account_signed_in?%>
						<a href="/organization_accounts/profile"> <%= Organization.find(current_organization_account.organization_id).name%></a>&nbsp;&nbsp;
						<a href="/signout" class="logout">LOGOUT</a>
					<% end %>
				<% end %>
				
			</div>
			</div>
			
			
			
			<% if user_signed_in? && current_user.admin?%>
			
			<div class="four columns" style="float:right" id="adminNav">
			<div class="row" style="background-color:#575758; color:white; font-size:1.3em; text-align:center; padding-top:7px;padding-bottom:7px;">
				<ul>
					<li><p style="color:red">Admin Links</p>
						<div>
						<ul>
							<li><a href="/blog_posts/new">New Blog Post</a></li>
							<li><a href="/organizations/new">New Organization</a></li>
							<li><a href="/programs/new">New Program</a></li>
							<li><a href="/organizations">All Organizations</a></li>
							<li><a href="/feedbacks">All Feedbacks</a></li>
							<li><a href="/flags">All Flags</a></li>
							<li><a href="/reviews">All Reviews</a></li>
							<li><a href="/new_reviews">All New Reviews</a></li>
							<li><a href="/users">All Users</a></li>
							<li><a href="/searches">All Searches</a></li>
							<li><a href="/organization_accounts">All Org Accounts</a></li>
						</ul>
						</div>
					</li>
				</ul>
				
			</div>
			</div>
			
			
			
			<% end %>
			
			
			<% if !organization_account_signed_in?%>
			<div class="four columns" style="float:right" id="orgLogin">
			<div class="row" style="background-color:#e67b5a; color:white; font-size:1.3em; text-align:center; padding-top:7px;padding-bottom:7px;">
					<ul>
						<li><p style="color:white">For Organizations</p>
							<div>
								<ul>
									<li><a href="#org-login-box" class="org-login-window">Login</a></li>
									<li><a href="#request-box" class="request-window">Make a Profile</a></li>
								</ul>
							</div>
						</li>
					</ul>
			</div>
			</div>
			<% end %>
			
			<div class="six columns" style="float:right">
				<% @search = Search.new%>
			<%= form_for @search do |f|%>
				<div style="margin-bottom:15px; float:right;">
			    <%= image_submit_tag "https://s3-us-west-1.amazonaws.com/volunteervoice/images/magGlass.png", :style => "margin-bottom:-57px", :id =>"searchbutton", :onmousedown => "this.className = 'shadowed'", :onmouseup => "this.className = 'nothing'"%></div>
				<div style="margin-top:40px; float:right; margin-right:5px">
				<input type="text" name="search[keywords]" id="searchfield"></div>
			<% end %>

		</div>
		</div>
		</header>
		</div>	
			
		<!-- end header-->

		<!-- Global Navigation-->
		
		<div class="container" id="globalNav">
		<div class="row">
		<nav class="nav">

		<ul>
		<li class="navs"><a href="/">home</a></li>
		<li class="navs" style="height:50px"><a id="shower">find a program</a>
			<div align="center">
			<ul>
				<li class="navs"><a href="/pages/program_browse">browse things to do</a></li>
				<li class="navs"><a href="/programs">browse places to go</a></li>
				<li class="navs"><a href="/pages/search_machine">use search machine</a></li>	
			</ul>
			</div>
		</li>
		<li class="navs"><a href="/reviews/new">write a review</a></li>
		<li class="navs"><a href= "/pages/blogs">resources</a></li>
		</ul>
		
		<div class="dropdown_background"></div>

		</nav></div>
		</div><!--end navigation-->	

<%= yield%>
</div>


