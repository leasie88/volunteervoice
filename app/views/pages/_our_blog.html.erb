

<table style="background-color:#E3E6D5" width="100%">
	<%if !(f.image.url == "/images/original/missing.png")%>
	<tr style="background-color:#E3E6D5"><td> <div align="center"><p style="margin-top:-10px"><%= image_tag f.square_image.url, :style => "height:100px; width:300px"%></p></div></td></tr>
	<% end %>
<tr style="background-color:#E3E6D5"><td><p style="margin-left:3%; margin-right:3%; margin-top:-10px; font-size:2em; line-height:1.1em" class="green"><a href="<%=f%>"> <%=f.title%></a></p></td></tr>
<tr style="background-color:#E3E6D5"><td><p style="margin-left:3%; margin-right:3%; margin-top:-10px" href= "#">Posted <%= f.published_at.strftime("%m/%e/%Y").gsub(/^0/,'')%></p></td></tr>
<tr style="background-color:#E3E6D5"><td><p style="margin-left:3%; margin-top:-15px; margin-right:3%"><%= f.truncated100.html_safe%> </p><br /> </td></tr>
</table>
