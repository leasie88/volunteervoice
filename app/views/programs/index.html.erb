<head>
    <% @nums = Array.new%>
	<% @progNums = Array.new%>
    <% @countries_with_programs.each do |f|%>
	<% @progNums << Program.where(:location => f).count unless f == ""%>
    <% @nums << ALLCOUNTRIES.index(f) unless (ALLCOUNTRIES.index(f).nil?)%>
    <% end %>
	
	
	<%= javascript_include_tag "/assets/jquery.js"%>


