$(document).ready(function() {
var	ones = 0;
var	twos = 0;
var	threes = 0;
var	fours = 0;
var	fives = 0;
var	sixs = 0;
var	sevens = 0;
var	eights = 0;
var	nines = 0;
var	tens = 0;
var	count = 0;
	$('#checkBefore').hide();
	$('#checkTerms').hide();
	$('a.terms-window').click(function() {

                //Getting the variable's value from a link 
		var termsBox = $(this).attr('href');

		//Fade in the Popup
		$(termsBox).fadeIn(300);

		//Set the center alignment padding + border see css style
		var popMargTop = ($(termsBox).height() + 24) / 2; 
		var popMargLeft = ($(termsBox).width() + 24) / 2; 

		$(termsBox).css({ 
			'margin-top' : -popMargTop,
			'margin-left' : -popMargLeft
		});

		// Add the mask to body
		$('body').append('<div id="mask" style="opacity:.5"></div>');
		$('#mask').fadeIn(300);

		return false;
	});

	// When clicking on the button close or the mask layer the popup closed
	$('a.close, #mask').live('click', function() { 
	  $('#mask , .terms-popup').fadeOut(300 , function() {
		$('#mask').remove();  
	}); 
	return false;
	});
	
	
	$('a.qMark-window').click(function() {

                //Getting the variable's value from a link 
		var qMarkBox = $(this).attr('href');

		//Fade in the Popup
		$(qMarkBox).fadeIn(300);

		//Set the center alignment padding + border see css style
		var popMargTop = ($(qMarkBox).height() + 24) / 2; 
		var popMargLeft = ($(qMarkBox).width() + 24) / 2; 

		$(qMarkBox).css({ 
			'margin-top' : -popMargTop,
			'margin-left' : -popMargLeft
		});

		// Add the mask to body
		$('body').append('<div id="mask" style="opacity:.5"></div>');
		$('#mask').fadeIn(300);

		return false;
	});

	// When clicking on the button close or the mask layer the popup closed
	$('a.close, #mask').live('click', function() { 
	  $('#mask , .qMark-popup').fadeOut(300 , function() {
		$('#mask').remove();  
	}); 
	return false;
	});
	
		$('a.allCompanies-window').click(function() {

	                //Getting the variable's value from a link 
			var allCompaniesBox = $(this).attr('href');

			//Fade in the Popup
			$(allCompaniesBox).fadeIn(300);

			//Set the center alignment padding + border see css style
			var popMargTop = ($(allCompaniesBox).height() + 24) / 2; 
			var popMargLeft = ($(allCompaniesBox).width() + 24) / 2; 

			$(allCompaniesBox).css({ 
				'margin-top' : -popMargTop,
				'margin-left' : -popMargLeft
			});

			// Add the mask to body
			$('body').append('<div id="mask" style="opacity:.5"></div>');
			$('#mask').fadeIn(300);

			return false;
		});

		// When clicking on the button close or the mask layer the popup closed
		$('a.close, #mask').live('click', function() { 
		  $('#mask , .allCompanies-popup').fadeOut(300 , function() {
			$('#mask').remove();  
		}); 
		return false;
		});
		
			$('a.addPhotos-window').click(function() {

		                //Getting the variable's value from a link 
				var addPhotosBox = $(this).attr('href');

				//Fade in the Popup
				$(addPhotosBox).fadeIn(300);

				//Set the center alignment padding + border see css style
				var popMargTop = ($(addPhotosBox).height() + 24) / 2; 
