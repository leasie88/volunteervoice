				$('body').append('<div id="mask" style="opacity:.5"></div>');
				$('#mask').fadeIn(300);

				return false;
			});

			// When clicking on the button close or the mask layer the popup closed
			$('a.close, #mask').live('click', function() { 
			  $('#mask , .addPhotos-popup').fadeOut(300 , function() {
				$('#mask').remove();  
			}); 
			return false;
			});
		
	});

	var preparation = "notClicked";
	var support = "notClicked";
	var impact = "notClicked";
	var overall = "notClicked";
	var structure = "notClicked";


	function nameFile() {
		var str="";
		str= $("#review_photo").val();
		document.getElementById('filename').value = str.substring(12, str.length);
	}

	function clickReviewPhoto() {
		$("#review_photo").click();
	}


	function beforeCheck() {
		$('#checkBefore').toggle();
	}

	function termsCheck() {
		$('#checkTerms').toggle();
	}

	function removeTwoHundred() {
		if ($("#theBody").value == "Minimum 200 characters" ) {
			$("#theBody").value = "";
	    }
	}

	function backToTwoHundred() {
		if ($("#theBody").attr("value") == "" ) {
			$("#theBody").attr("value", "Minimum 200 characters");
	    }
	}

	function removePut() {
		if ($("#put").attr("value") == "  Put the organization's name here" ) {
			$("#put").attr("value", "");
	    }
	}

	function backToPut() {
		if ($("#put").attr("value") == "" ) {
			$("#put").attr("value", "  Put the organization's name here");
	    }
	}



	function setClicked(theVar) {
		if (theVar == 'preparation') {
			preparation = "clicked";
		}
		if (theVar == 'support') {
			support = "clicked";
		}
		if (theVar == 'impact') {
			impact = "clicked";
		}
		if (theVar == 'structure') {
			structure = "clicked";
		}
		if (theVar == 'overall') {
			overall = "clicked";
		}
	}


	function showRemove() {
		$("#removeThePhoto").toggle();
	}

	function changeCount(field) {
	if (field == "one") {
		if $("#"+field) {
			ones = 1;
		} else {
			ones = 0;
		}
	} else if (field == "two") {
		if $("#"+field) {
			twos = 1;
		} else {
			twos = 0;
		}
	} else if (field == "three") {
		if $("#"+field) {
			threes = 1;
		} else {
			threes = 0;
		}
	} else if (field == "four") {
		if $("#"+field) {
			fours = 1;
		} else {
			fours = 0;
		}
	} else if (field == "five") {
		if $("#"+field) {
			fives = 1;
		} else {
			fives = 0;
		}
	} else if (field == "six") {
		if $("#"+field) {
			sixs = 1;
		} else {
			sixs = 0;
		}
	} else if (field == "seven") {
		if $("#"+field) {
			sevens = 1;
		} else {
			sevens = 0;
		}
	} else if (field == "eight") {
		if $("#"+field) {
			eights = 1;
		} else {
			eights = 0;
		}
	} else if (field == "nine") {
		if $("#"+field) {
			nines = 1;
		} else {
			nines = 0;
		}
	} else if (field == "ten") {
		if $("#"+field) {
			tens = 1;
		} else {
			tens = 0;
		}
	}
		count = ones + twos + threes + fours + fives + sixs + sevens + eights + nines + tens;
		$("#total_count").attr("value", count);

	}
	
	function removePhoto(field) {
		$("#"+field).attr("value", "");
	}
	</script>








</head>


<body>
<div class="container" style="color:#575758">
	<div class="row" style="margin-top:50px;margin-bottom:20px" align="center">
<%=image_tag "https://s3-us-west-1.amazonaws.com/volunteervoice/images/writeReview.png"%>
</div>
<br />


<% organizations_array = Organization.all.map { |org| [org.name, org.id] } %>
<% if @review.organization_id.nil?%>
<% programs_array = Program.all.map { |p| [p.name, p.id]}%>
<% else %>
<% programs_array = Program.where(:organization_id => @review.organization_id).map { |p| [p.name, p.id]}%>
<% end %>
<%= form_for @review do |f| %>
