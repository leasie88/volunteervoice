	<div style="margin-top: -30px; height:30px;">
				       
				<% if @review.organization_id.nil?%>
					
						<div width="200px">
						<input id="tags" style="float:right; margin-right:10px; margin-top:5px; width:200px; height:20px" name="review[organization_name]"></div>
				
				<% else %>
				<%= Organization.find(@review.organization_id).name%>
				<% end %>

				</div>
				<div style="margin-left:55%; margin-top:3px"><a href="#allCompanies-box" id="allCompaniesShow" class="allCompanies-window">List All</a> | <a href="/new_reviews/new" id="reviewNew">Review New Company</a>
					</div>
			</section><!--end selectCompanyBox-->
		</div>
		
	
		


	<div class="six columns" style=" margin-top:5px; font-family:ArchitectsDaughterRegular; font-size:0.85em; color:#62acbd;">

<article class="selectCompanyBlurb"><!-- select a comany blurb-->
