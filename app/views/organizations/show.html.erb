<head>
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.3/jquery.min.js"></script>

<%= stylesheet_link_tag "/assets/jquery.rating.css"%>

<%= stylesheet_link_tag "/assets/smartpaginator.css"%>

<%= javascript_include_tag "/assets/smartpaginator.js"%>


<script src='http://jquery-star-rating-plugin.googlecode.com/svn/trunk/jquery.MetaData.js' type="text/javascript" language="javascript"></script>
<script src='http://jquery-star-rating-plugin.googlecode.com/svn/trunk/jquery.rating.js' type="text/javascript" language="javascript"></script>

<script src='http://jquery-fyneworks.googlecode.com/svn/trunk/documentation/documentation.js' type="text/javascript"></script>
<% @star_count = 1%>

<p id="notice"><%= notice %></p>
<%@organization.page_views = (@organization.page_views + 1)%>
<% @organization.save! %>

<script type="text/javascript">
$(document).ready(function () {
	$('#reviewws_pager').smartpaginator({ totalrecords: <%=@results.count%>, recordsperpage: 1, datacontainer: 'results', dataelement: 'div', initval: 0, next: 'Next', prev: 'Prev', first: 'First', last: 'Last', theme: 'black' });

});

</script>

</head>
	
<div class="container" style="margin-left:20px; margin-right:20px">
    <div class="row">
	
	<div id="show-contents" class="contents">
		<div style="height: 30px; margin-top:40px; margin-right:50px">
            <ul class="tab">
				<li style="background-color: #E9ECE9; position:absolute; right:250px;top:196px; border-top:1px solid #575758; border-right:1px solid #575758; border-left:1px solid #575758; height:20px; width:100px; font-size:1.3em; text-align:center" id="5"><a href="/organizations/<%= @organization.id%>">Profile</a></li>
				<li style="background-color: #C1C4C2; position:absolute;right:120px;top:196px; border-top:1px solid #575758; border-right:1px solid #575758; border-left:1px solid #575758; height:19px; width:100px; font-size:1.3em; text-align:center" id="6"><a href="/organizations/<%= @organization.id%>/programs">Programs</a></li>
            </ul>
        </div>
		
	<%= render :partial => "show_contents", :locals => {:organization => @organization}%>
	
	<div class="row">
			<div class="twelve columns" style="float:center; text-align:center">
			<p style="font-family:chunkFiveRegular;font-size:3em; line-height:1.7em; margin-top:25px; margin-bottom:25px; color: #62acbd">Reviews & Insight</p>
			</div>
		</div>
		<div class="row">
			<div class="fivecolumns">
			<p style="font-size:1.5em; color:#575758; line-height:1.1em; margin-left:40px; font-weight:600"> List By:</p>	

				<form accept-charset="UTF-8" action="sorted_results" method="get"><div style="margin:0;padding:0;display:inline"><input name="utf8" type="hidden" value="&#x2713;" /></div>
					<div style="position:relative; left:18%;top:-25px; height:30px; width:228px; overflow:hidden; border-right:1px solid #575758; border-left:1px solid #575758">
				<select id="sort" name="sort"  style="position:absolute; left:-5px; width:255px; height:30px; background: url(https://s3-us-west-1.amazonaws.com/volunteervoice/images/arrowReviews.png) no-repeat right; background-color:white; border: 1px solid #575758" onchange="this.form.submit()"><option value="ratinghigh">Rating - High</option>
				<option value="ratinglow">Rating - Low</option>
				<option value="alphabetical">Alphabetical</option>
				<option value="pricelow">Price - Low</option>
				<option value="pricehigh">Price - High</option></select>
								<input id="resulting_ids" name="resulting_ids" type="hidden" value="2 3" />
								<input id="type" name="type" type="hidden" value="Program" />
								</div>
				</form>
			</div>

		</div>

	<div class="container">
		<div class="row">
	<div id="results" style="margin-left:20px; margin-right:20px">

	
				<% @results.each do |f| %>
				<div>
					<%= render :partial => "results", :locals => {:f => f}%>
				</div>
				<% end %>
		
			
			<div id="reviewws_pager" style=" width:80%">
			</div>
	
</div>
</div>
</div>
</div>
</div>

<div class = "container">
<div class = "row">
	<div id= "four columns" align="center" style="margin-top:20px; margin-bottom:5px">
		<% if !(current_user.nil?) && current_user.admin? %>
		<%= link_to "edit", :controller => "organizations", :action => "edit", :id => @organization.id%>
		 <%= @organization.page_views%>
		<% end %>
	</div>
</div>
</div>

	
		


<style type="text/css">



    .header
    {
        background-color: #4f7305;
        color: White;
    }
   
    ul.tab
    {
        list-style: none;
        margin: 0;
        padding: 0;
		float: right;
    }
    ul.tab li
    {
        display: inline;
        padding: 10px;
        color: #575758;
        cursor: pointer;
    }
</style>
